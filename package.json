{
  "name": "generator-maso",
  "version": "1.0.0",
  "description": "Opinionated boilerplate for a React application",
  "scripts": {
    "compile": "babel --presets es2015,stage-0 --plugins transform-runtime -d app/ --ignore src/templates src/; cp -R src/templates app",
    "lint": "eslint --debug src/",
    "prepublish": "rm -rf app/; npm run compile",
    "test": "eslint --debug src/; ava"
  },
  "files": [
    "app"
  ],
  "author": "Benjamin Goh <masotime@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "bluebird": "^3.3.5",
    "yeoman-generator": "^0.22.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/masotime/generator-maso.git"
  },
  "keywords": [
    "yeoman-generator"
  ],
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.3",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^2.8.0"
  }
}
